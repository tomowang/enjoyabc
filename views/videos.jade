extends layout

block content
  .page.thirdary
    include includes/header
    .page-region
      .page-region-content
        each v in videos
          .image-container.bg-color-blue51.place-left
            img(src="/downloads/#{v.uuid}.jpg")
            .overlay #{v.title}
              | <br>
              | #{v.date}
            a.icon-download-2.bottom-right(href="/downloads/#{v.uuid}.#{v.ext}", target="_blank")
        //-a#upload.place-left.bg-color-blue51.icon-upload-3(href="#")
    //-include includes/footer
